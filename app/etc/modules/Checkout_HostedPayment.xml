<config>
    <modules>
<!-- declare Company_Namespace module -->
<!-- Copy this file here on install module: app/etc/modules/Checkout_HostedPayment.xml -->
        <Checkout_HostedPayment>
<!-- this is an active module -->
            <active>true</active>
<!-- this module will be located in app/code/local code pool -->
            <codePool>local</codePool>
<!-- specify dependencies for correct module loading order -->
            <depends>
                <Mage_Payment />
            </depends>
        </Checkout_HostedPayment>
    </modules>
</config>